{
  "name": "herobot",
  "version": "1.0.0",
  "description": "A discord bot to play RPG",
  "main": "./src/bot.ts",
  "dependencies": {
    "@types/node": "^10.12.3",
    "discord.js": "^11.4.2",
    "firebase": "^5.5.7",
    "firebase-tools": "^6.0.1",
    "gulp": "^3.9.1",
    "https-proxy-agent": ">=2.2.0",
    "nodemon": "^1.18.6"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.5",
    "chai": "^4.2.0",
    "gulp-typedoc": "^2.2.0",
    "mocha": "^5.2.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.3",
    "typings": "^2.1.1",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2"
  },
  "scripts": {
    "test": "mocha -r ts-node/register tests/**/*.spec.ts",
    "build-watch": "tsc --watch",
    "build": "tsc -p .",
    "bot": "node build/src/bot.js",
    "build:test": "webpack -p --config ./src/environment/webpack.config.ts",
    "build-docs": "rimraf docs && typedoc --options typedoc.json src/",
    "build:prod": "webpack -p --define process.env.NODE_ENV='\"production\"' --config ./src/environment/webpack.config.ts",
    "postinstall": "tsc -p ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lgmagalhaes88/HeroBot.git"
  },
  "author": "lgmagalhaes88",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lgmagalhaes88/HeroBot/issues"
  },
  "homepage": "https://github.com/lgmagalhaes88/HeroBot#readme"
}
