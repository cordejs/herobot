{
  "name": "herobot",
  "version": "1.0.0",
  "description": "A discord bot to play RPG",
  "main": "./src/bot.ts",
  "dependencies": {
    "@types/node": "^10.12.12",
    "discord.js": "^11.4.2",
    "event-stream": "^3.3.4",
    "firebase": "^5.7.0",
    "firebase-tools": "^6.1.2",
    "nodemon": "^1.18.6"
  },
  "devDependencies": {
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.3",
    "typings": "^2.1.1"
  },
  "scripts": {
    "build-watch": "tsc --watch",
    "build": "tsc -p .",
    "bot": "node build/src/bot.js",
    "build-docs": "rimraf docs && typedoc --options typedoc.json src/",
    "build:prod": "webpack -p --define process.env.NODE_ENV='\"production\"' --config ./src/environment/webpack.config.ts",
    "postinstall": "tsc -p .",
    "heroku": "heroku local worker"
  },
  "keywords": [
    "heroku"
  ],
