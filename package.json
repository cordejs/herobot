{
  "name": "herobot",
  "version": "0.1.0",
  "description": "A discord bot to play RPG",
  "main": "./src/bot.ts",
  "dependencies": {
    "@types/node": "11.9.4",
    "cloc": "^2.4.0",
    "discord.js": "^11.4.2",
    "dotenv": "^6.2.0",
    "event-stream": "4.0.1",
    "nodemon": "1.18.10",
    "pg": "^7.8.0",
    "reflect-metadata": "^0.1.10",
    "typeorm": "0.2.13"
  },
  "devDependencies": {
    "husky": "^1.3.1",
    "jasmine-core": "^3.3.0",
    "karma": "^4.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^2.0.1",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.3",
    "typings": "^2.1.1"
  },
  "scripts": {
    "build-watch": "tsc --watch",
    "build": "tsc -p .",
    "bot": "node build/src/bot.js",
    "build-docs": "typedoc --out ./doc",
    "build:prod": "node -r build/config.js",
    "postinstall": "tsc -p .",
    "count": "cloc src/",
    "install-hooks": "symlinks.sh",
    "start": "ts-node src/index.ts",
    "ignore-reset": "bash ignore.sh",
    "commit": "commit.sh",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migrations-run": "typeorm migration:run",
    "migration-revert": "typeorm migration:revert",
    "migration-create": "typeorm migration:create -n migration",
    "prettier-watch": "onchange '**/*.js' -- prettier --write {{changed}}",
    "release": "bash release.sh",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "pretty-quick": "pretty-quick"
  },
  "keywords": [
    "heroku"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lgmagalhaes88/HeroBot.git"
  },
  "author": "lgmagalhaes88",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lgmagalhaes88/HeroBot/issues"
  },
  "engines": {
    "node": "10.x"
  },
  "homepage": "https://github.com/lgmagalhaes88/HeroBot#readme"
}
